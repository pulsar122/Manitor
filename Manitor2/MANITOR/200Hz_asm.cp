#include <XBRA.h>extern C_CALL void my_200Hz(void);	/* Codewarrior wants function for DC.L <address> */extern XBRA XBRA_200Hz;static void asm my_200Hz_interrupt(){entry extern XBRA_200Hz	dc.l 'XBRA'	dc.l 'Mntr'old_200Hz:	dc.l 0new_200Hz:	dc.w 0x23F8,0x4ba			// move.l 0x4ba,my_200Hz	dc.l my_200Hz//	addq.l #1,my_200Hz  ohne diese Zeile hinkt mein Counter stets einen Tick hinterher - egal!// besser wäre sowieso, den Counter in ein Register zu laden, dort um eins zu erhöhen// und erst dann in die my_200Hz-Variable zu schreiben!	move.l old_200Hz,-(sp)	rts}