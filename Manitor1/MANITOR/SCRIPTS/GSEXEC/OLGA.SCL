main ()
{
  if ( address("manitor") )
  {
    vdi(0);           # VDI aus
    gemdos(0);        # Gemdos aus
    xbios(0);         # Xbios aus
    bios(0);          # Bios aus

    aes(1, 1);        # AES an, Returns an
    msg(1, 1, 1, 0);  # Send an, Received an, Rcv nur vom Screenmanager, pnam aus
    hide("");         # Alle AES-Funktionen abschalten

    hide_msg("");     # Alle Messages abschalten
    show_msg("OLE");  # OLE an
    show_msg("OLGA"); # OLGA an

    activate();       # Manitor aktivieren
  }
}
